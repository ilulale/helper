abstract class Shape
{
	int a,l,b;
	Shape()
	{
		l=3;
		b=2;
	}

	abstract public int calculateArea();
	
	public void display()
	{
		System.out.println(a);
	}
}

public class Rectangle extends Shape
{
	
	public static void main(String args[])
	{
		 		
		Rectangle r = new Rectangle();
		r.calculateArea();
		r.display();
	}
	public int calculateArea()
	{
		a = l * b ;
		return a;
	}
}