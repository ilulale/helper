import java.io.*;
import java.util.*;

class math
{
	int real, imaginary;
	
	math(int r, int i)
	{
		this.real = r;
		this.imaginary = i;
	}
	public static void main(String[] args)
	{
		math n1 = new math(2, 4);
		math n2 = new math(3, 5);
		math temp, temp1, temp2;
		
		temp = add(n1, n2);
		temp1 = sub(n1,n2);
		temp2 = mul(n1,n2);
		
        System.out.printf("Sum = %d + %di\n", temp.real, temp.imaginary);
		System.out.printf("Sub = %d - %di\n", temp1.real, temp1.imaginary);
		System.out.printf("Mul = %d * %di", temp2.real, temp2.imaginary);
    }
	
	public static math add(math n1, math n2)
    {
		math temp = new math(0, 0);
		temp.real = n1.real + n2.real;
		temp.imaginary = n1.imaginary + n2.imaginary;
		return(temp);
    }
	
	public static math sub(math n1, math n2)
    {
		math temp1 = new math(0, 0);
		temp1.real = n1.real - n2.real;
		temp1.imaginary = n1.imaginary - n2.imaginary;
		return(temp1);
    }
	
	public static math mul(math n1, math n2)
    {
		math temp2 = new math(0, 0);
		temp2.real = n1.real * n2.real;
		temp2.imaginary = n1.imaginary * n2.imaginary;
		return(temp2);
    }
}